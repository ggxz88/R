library(ggplot2)

ggplot(data = mpg, aes(x = displ, y = hwy)) + geom_point() + 
xlim(3, 6) + 
ylim(10, 30)

ggplot(data = mpg, aes(x = cty, y = hwy)) + geom_point() # 혼자서 해보기 1

ggplot(data = midwest, aes(x = poptotal, y = popasian)) + geom_point() + 
  xlim(0, 500000) + 
  ylim(0, 10000) # 혼자서 해보기 2
